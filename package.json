{
  "name": "boilerplate-type-react",
  "version": "0.1.0",
  "description": "typescriptとreactのテンプレート",
  "main": "dist/app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "dev": "ts-node src/app.ts",
    "dev:watch": "ts-node-dev --respawn src/app.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "lint": "npm-run-all eslint check-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onesword0618/boilerplate-type-react.git"
  },
  "keywords": [
    "template"
  ],
  "author": "onesword0618",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onesword0618/boilerplate-type-react/issues"
  },
  "homepage": "https://github.com/onesword0618/boilerplate-type-react#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "7.12.1",
    "@types/eslint": "7.2.4",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/prettier": "2.1.5",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-test-renderer": "16.9.3",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-jest": "26.6.3",
    "eslint": "7.13.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "http-server": "0.12.3",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "pretty-format": "26.6.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0",
    "tslib": "2.0.3",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  }
}